$sql = "SELECT DISTINCT (user_id) FROM `table 1`\n"

    . "WHERE user_id IN (SELECT DISTINCT (user_id) FROM `table 1` WHERE (is_new_user = 0) AND (login_time BETWEEN \'2015-12-20 00:00:00\' AND \'2015-12-27 00:00:00\'))";

