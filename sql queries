// NEW USERS IN A WEEK WHERE ALLEVENTS WORKS
$sql = "SELECT * FROM `table 1` WHERE (is_new_user = 1) AND (login_time BETWEEN \'2015-11-23 18:36:22\' AND \'2015-11-24 00:00:00\')";

